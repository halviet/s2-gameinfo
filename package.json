{
  "name": "s2-gameinfo",
  "type": "module",
  "version": "1.0.2",
  "private": false,
  "description": "Bidirectional parser for Source 2 engine gameinfo.gi files. Parse Valve game configurations to JSON and compose them back.",
  "keywords": [
    "typescript",
    "source2",
    "gameinfo"
  ],
  "author": "halviet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halviet/s2-gameinfo.git"
  },
  "bugs": {
    "type": "git",
    "url": "https://github.com/halviet/s2-gameinfo"
  },
  "homepage": "https://github.com/halviet/s2-gameinfo#readme",
  "LICENSE": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
